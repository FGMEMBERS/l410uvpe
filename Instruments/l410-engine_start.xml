<actions>
  <action>
   <name>start lh engine</name>
   <button>0</button>
   <x>-24</x>
   <y>-92</y>
   <w>24</w>
   <h>24</h>
   <binding>
      <command>property-assign</command>
      <property>/controls/switches/start-engine1-sw</property>
      <value type="bool">true</value>
  </binding>
  <binding>
   <command>property-assign</command>
   <property>/controls/engines/engine[0]/starter</property>
   <value type="bool">true</value>
  </binding>
  <mod-up>
   <binding>
      <command>property-assign</command>
      <property>/controls/switches/start-engine1-sw</property>
      <value type="bool">false</value>
  </binding>
  <binding>
   <command>property-assign</command>
