<!--start lh-->
 <layer>
   <name>start engine lh</name>
   <type>switch</type>
   <layer>
     <condition>
       <property>/controls/switches/start-engine1-sw</property>
     </condition>
     <name>on</name>
     <texture>
     <path>Aircraft/l410uvpe/Instruments/Textures/l410-prislus.rgb</path>
     <x1>0.0</x1>
     <x2>0.14118</x2>
     <y1>0.48438</y1>
     <y2>0.61119</y2>
     </texture>
     <w>15</w>
     <h>15</h>
   </layer>
   <layer>
     <name>off</name>
     <texture>
     <path>Aircraft/l410uvpe/Instruments/Textures/l410-prislus.rgb</path>
     <x1>0.0</x1>
     <x2>0.14118</x2>
     <y1>0.34902</y1>
     <y2>0.47451</y2>
     </texture>
     <w>15</w>
     <h>15</h>
   </layer>
   <transformations>
      <transformation>
        <type>x-shift</type> 
        <offset>-12</offset> 
      </transformation>
      <transformation>
        <type>y-shift</type> 
        <offset>-80</offset> 
      </transformation>
   </transformations>
 </layer>
 
