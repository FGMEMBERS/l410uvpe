<!-- bulbs  -->

<!-- aut. bank control (yellow) -->
 <layer>
   <name>autbank_yellow_wd</name>
   <type>switch</type>
    <layer>
     <condition>
	  <and>
		 <greater-than>
           <property>/systems/electrical/outputs/cwd-airframe</property>
		   <value>10</value>
		 </greater-than>
         <or>
           <property>/controls/switches/chck-airframe-sw</property>
           <property>/instrumentation/warn-disp/wd-autbank-y</property>
         </or>
	  </and>
     </condition>
     <name>on</name>
     <texture>
     <path>Aircraft/l410uvpe/Instruments/Textures/l410-svet1.rgb</path>
     <x1>0.5214</x1>
     <x2>0.6953</x2>
     <y1>0.6836</y1>
     <y2>0.7598</y2>
     </texture>
     <w>46</w>
     <h>22</h>
	 <emissive>true</emissive>
    </layer>
    <layer>
     <name>off</name>
     <texture>
     <path>Aircraft/l410uvpe/Instruments/Textures/l410-svet1.rgb</path>
     <x1>0.5214</x1>
     <x2>0.6953</x2>
     <y1>0.6074</y1>
     <y2>0.6836</y2>
     </texture>
     <w>46</w>
     <h>22</h>
   </layer>
   <transformations>
    <transformation>
    <type>x-shift</type>
    <offset>-98</offset>
    </transformation>
    <transformation>
    <type>y-shift</type>
    <offset>+25</offset>
    </transformation>
   </transformations>
  </layer>

