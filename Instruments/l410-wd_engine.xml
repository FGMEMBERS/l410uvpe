<!-- 1st line -->
<!-- fire -->
    
 <layer>
   <name>fire_wd</name>
   <type>switch</type>
    <layer>
     <condition>
	  <and>
		 <greater-than>
           <property alias="../../../../../../../params/wde-el-input"/>
		   <value>10</value>
		 </greater-than>
         <or>
           <property alias="../../../../../../../params/wde-chck-sw"/>
           <property alias="../../../../../../../params/wde-fire"/>
         </or>
	  </and>
     </condition>
     <name>on</name>
     <texture>
     <path>Aircraft/l410uvpe/Instruments/Textures/l410-svet1.rgb</path>
     <x1>0.0</x1>
     <x2>0.1718</x2>
     <y1>0.0762</y1>
     <y2>0.1504</y2>
     </texture>
     <w>46</w>
     <h>22</h>
	 <emissive>true</emissive>
    </layer>
    <layer>
     <name>off</name>
     <texture>
     <path>Aircraft/l410uvpe/Instruments/Textures/l410-svet1.rgb</path>
     <x1>0.0</x1>
     <x2>0.1718</x2>
     <y1>0.0</y1>
     <y2>0.0762</y2>
     </texture>
     <w>46</w>
     <h>22</h>
   </layer>
   <transformations>
    <transformation>
    <type>x-shift</type>
    <offset>-98</offset>
    </transformation>
    <transformation>
    <type>y-shift</type>
    <offset>+25</offset>
    </transformation>
   </transformations>
  </layer>

