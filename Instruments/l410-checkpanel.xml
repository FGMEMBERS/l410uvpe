      <transformation>
        <type>x-shift</type> 
        <offset>17</offset> 
      </transformation>
      <transformation>
        <type>y-shift</type> 
        <offset>40</offset> 
      </transformation>
      <transformation>
        <type>rotation</type> 
        <property>/controls/switches/chck-extinguish-eng-sw</property>
        <scale>30</scale>
      </transformation>
    </transformations>
