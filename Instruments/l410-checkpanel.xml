  <layer>
    <name>zarovka pyro</name>
    <type>switch</type>
    <layer>
      <name>on</name>
      <condition>
	    <and>
		  <greater-than>
            <property>/systems/electrical/outputs/bus28v</property>
			<value>10</value>
		  </greater-than>
          <property>/instrumentation/warn-disp/wd-extinguish-eng</property>
		</and>
      </condition>
      <texture>
        <path>Aircraft/l410uvpe/Instruments/Textures/l410-prislus.rgb</path>
        <x1>0.67843</x1>
        <x2>0.81961</x2>
        <y1>0.004</y1>
        <y2>0.141</y2>
      </texture>
      <w>24</w>
      <h>32</h>
	  <emissive>true</emissive>
    </layer>
    <layer>
      <name>off</name>
      <texture>
        <path>Aircraft/l410uvpe/Instruments/Textures/l410-prislus.rgb</path>
        <x1>0.67843</x1>
        <x2>0.81961</x2>
        <y1>0.1451</y1>
        <y2>0.28235</y2>
      </texture>
      <w>24</w>
      <h>32</h>
    </layer>
    <transformations>
      <transformation>
        <type>x-shift</type> 
        <offset>-32</offset> 
      </transformation>
      <transformation>
        <type>y-shift</type> 
        <offset>30</offset> 
      </transformation>
    </transformations>
  </layer>
