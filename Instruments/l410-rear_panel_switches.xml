  <actions>
    <action>
        <name>Steering</name>
        <button>0</button>
        <x>-33</x>
        <y>-66</y>
        <w>36</w>
        <h>36</h>
        <binding>
            <command>property-adjust</command>
            <property>/controls/switches/full-steering-sw</property>
            <max>2</max>
            <min>0</min>
            <step>1</step>
        </binding>
    </action>
    <action>
        <name>Steering</name>
        <button>0</button>
        <x>-33</x>
        <y>-33</y>
        <w>36</w>
        <h>36</h>
