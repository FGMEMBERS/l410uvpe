<!-- baterie -->
    
 <layer>
   <name>battery_wd</name>
   <type>switch</type>
    <layer>
     <condition>
	  <and>
		 <greater-than>
           <property>/systems/electrical/outputs/cwd-electrical-spec</property>
		   <value>10</value>
		 </greater-than>
         <or>
           <property>/controls/switches/chck-electro-sw</property>
           <property>/instrumentation/warn-disp/wd-battery</property>
         </or>
	   </and>
     </condition>
     <name>on</name>
     <texture>
     <path>Aircraft/l410uvpe/Instruments/Textures/l410-svet.rgb</path>
     <x1>0.0</x1>
     <x2>0.17578</x2>
     <y1>0.3262</y1>
     <y2>0.4</y2>
     </texture>
     <w>46</w>
     <h>22</h>
	 <emissive>true</emissive>
    </layer>
    <layer>
     <name>off</name>
     <texture>
     <path>Aircraft/l410uvpe/Instruments/Textures/l410-svet.rgb</path>
     <x1>0.0</x1>
     <x2>0.17578</x2>
     <y1>0.25</y1>
     <y2>0.32223</y2>
     </texture>
     <w>46</w>
     <h>22</h>
   </layer>
   <transformations>
    <transformation>
    <type>x-shift</type>
    <offset>+49</offset>
    </transformation>
    <transformation>
    <type>y-shift</type>
    <offset>-50</offset>
    </transformation>
   </transformations>
  </layer>
  
