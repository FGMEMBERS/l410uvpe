<PropertyList>

 <fx>
  <fire-bell>
    <name>fire-bell</name>
	<path>Aircraft/l410uvpe/Sounds/l410-bell2.wav</path>
    <condition>
      <and>
	    <greater-than>
          <property>/systems/electrical/outputs/bus28v</property>
          <value>10</value>
        </greater-than>
 	    <or>
		  <property>/instrumentation/fire-det/fire-englh</property>
		  <property>/instrumentation/fire-det/fire-engrh</property>
		  <property>/instrumentation/fire-det/fire-abc</property>
		  <property>/instrumentation/fire-det/fire-fbc</property>
        </or>
	  </and>
    </condition>
    <mode>looped</mode>
    <volume>
      <max>1.5</max>
      <min>1.5</min>
    </volume>
    <pitch>
      <max>0.5</max>
      <min>0.5</min>
      <value>0.5</value>
    </pitch>
    <position>
     <x>0</x>
     <y>1</y>
     <z>0</z>
    </position>
  </fire-bell>

  <ap-beep>
    <name>ap-beep</name>
   <path>Aircraft/l410uvpe/Sounds/l410-beep1.wav</path>
    <condition>
      <and>
	    <greater-than>
          <property>/systems/electrical/outputs/bus28v</property>
          <value>10</value>
        </greater-than>
 	    <or>
		  <property>/instrumentation/autopilot/as-alert-flag</property>
        </or>
	  </and>
    </condition>
   <mode>once</mode>
   <volume>
      <max>0.5</max>
      <min>0.5</min>
   </volume>
   <pitch>
      <max>0.2</max>
      <min>0.2</min>
      <value>0.2</value>
   </pitch>
   <position>
    <x>0</x>
    <y>1</y>
    <z>0</z>
   </position>
  </ap-beep>

  <ap-disconnect>
    <name>ap-disconnect</name>
    <path>Aircraft/l410uvpe/Sounds/l410-ap-disconnect.wav</path>
    <condition>
      <and>
	    <greater-than>
	      <property>/systems/electrical/outputs/bus28v</property>
	      <value>10</value>
	    </greater-than>
	    <not>
          <property>/instrumentation/autopilot/mode-ap-on</property>
        </not>
      </and>
    </condition>
    <position>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </position>
  </ap-disconnect>

  <ap-test>
    <mode>looped</mode>
    <name>ap-test</name>
    <path>Aircraft/l410uvpe/Sounds/l410-ap-disconnect.wav</path>
    <condition>
      <property>/instrumentation/autopilot/test-in-progress</property>
    </condition>
    <position>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </position>
  </ap-test>


  <beep>
   <name>xxx</name>
   <path>Aircraft/l410uvpe/Sounds/l410-beep1.wav</path>
   <condition>
     <property>/instrumentation/altimeter-rad/signal-on</property>
   </condition>
   <mode>once</mode>
   <volume>
      <max>0.5</max>
      <min>0.5</min>
   </volume>
   <pitch>
      <max>0.2</max>
      <min>0.2</min>
      <value>0.2</value>
   </pitch>
   <position>
    <x>0</x>
    <y>1</y>
    <z>0</z>
   </position>
  </beep>
  
  <beep>
   <name>xxx1</name>
   <path>Aircraft/l410uvpe/Sounds/l410-beep1.wav</path>
   <condition>
     <and>
	 <greater-than>
		 <property>/systems/electrical/outputs/bus28v</property>
		 <value>10</value>
	 </greater-than>
     <or>
       <property>/instrumentation/warn-disp/wd-bank-l</property>
       <property>/instrumentation/warn-disp/wd-bank-r</property>
       <property>/instrumentation/warn-disp/wd-extendgear</property>
       </or>
	 </and>
   </condition>
   <mode>looped</mode>
   <volume>
      <max>0.5</max>
      <min>0.5</min>
   </volume>
   <pitch>
      <max>0.2</max>
      <min>0.2</min>
      <value>0.2</value>
   </pitch>
   <position>
    <x>0</x>
    <y>1</y>
    <z>0</z>
   </position>
  </beep>

<!-- ENGINE + PROPELLER -->

  <engine>
   <name>propeller_l</name>
   <path>Aircraft/l410uvpe/Sounds/propeller.wav</path>
   <mode>looped</mode>
   <condition>
     <greater-than>
       <property>/engines/engine[0]/thruster/rpm</property>
       <value>100</value>
     </greater-than>
   </condition>
   <volume>
    <property>/engines/engine[0]/thruster/noise-coef</property>
    <type>abs</type>
    <factor type="double">1</factor>
    <offset type="double">0</offset>
   </volume>
<!--   <volume>
    <property>/sim/current-view/internal</property>
    <offset>0.8</offset>
    <factor>-0.5</factor>
   </volume> -->
   <pitch>
    <property>/engines/engine[0]/thruster/rpm</property>
    <type>abs</type>
    <factor>0.0005</factor>
    <offset>0.0</offset>
   </pitch> 
   <position>
    <x>-2.71198</x>
    <y>0.827244</y>
    <z>0.210361</z>
   </position>
  </engine>

  <engine>
   <name>engine_l</name>
   <path>Aircraft/l410uvpe/Sounds/turbine.wav</path>
