            <key n="70">
                <name>F</name>
                <desc>Cutoff on Selected Engine(s)</desc>
                <binding> 
                    <condition>
                        <property>/sim/input/selected/engine[0]</property>
                    </condition>
                    <command>property-assign</command>
                    <property>/controls/engines/engine[0]/cutoff-cmd</property>
                    <value type="bool">true</value>
                </binding>
                <binding>
                    <condition>
                        <property>/sim/input/selected/engine[1]</property>
                    </condition>
                    <command>property-assign</command>
                    <property>/controls/engines/engine[1]/cutoff-cmd</property>
                    <value type="bool">true</value>
                </binding>
            </key>

            <key n="102">
                <name>f</name>
                <desc>Not Cutoff on Selected Engine(s)</desc>
                <binding>
                    <condition>
                        <property>/sim/input/selected/engine[0]</property>
                    </condition>
                    <command>property-assign</command>
                    <property>/controls/engines/engine[0]/cutoff-cmd</property>
                    <value type="bool">false</value>
                </binding>
                <binding>
                    <condition>
                        <property>/sim/input/selected/engine[0]</property>
                    </condition>
                    <command>property-assign</command>
                    <property>/controls/engines/engine[0]/cutoff-emerg-cmd</property>
                    <value type="bool">false</value>
                </binding>
                <binding>
                    <condition>
                        <property>/sim/input/selected/engine[1]</property>
                    </condition>
                    <command>property-assign</command>
                    <property>/controls/engines/engine[1]/cutoff-cmd</property>
                    <value type="int">0</value>
                </binding>
                <binding>
                    <condition>
                        <property>/sim/input/selected/engine[1]</property>
                    </condition>
                    <command>property-assign</command>
                    <property>/controls/engines/engine[1]/cutoff-emerg-cmd</property>
                    <value type="int">0</value>
                </binding>
            </key>
  
            <key n="6">
                <name>ctrlF</name>
                <desc>Emergency Cutoff on Selected Engine(s)</desc>
                <binding>
                    <condition>
                        <property>/sim/input/selected/engine[0]</property>
                    </condition>
                    <command>property-assign</command>
                    <property>/controls/engines/engine[0]/cutoff-emerg-cmd</property>
                    <value type="int">1</value>
                </binding>
                <binding>
                    <condition>
                        <property>/sim/input/selected/engine[1]</property>
                    </condition>
                    <command>property-assign</command>
                    <property>/controls/engines/engine[1]/cutoff-emerg-cmd</property>
                    <value type="int">1</value>
                </binding>
            </key>

            <key n="92">
                <name>\</name>
                <desc>Reverser on Selected Engine(s)</desc>
                <binding>
                    <condition>
                        <property>/sim/input/selected/engine[0]</property>
                    </condition>
                    <command>property-assign</command>
                    <property>/controls/engines/engine[0]/reverser</property>
                    <value type="bool">true</value>
                </binding>
                <binding>
                    <condition>
                        <property>/sim/input/selected/engine[1]</property>
                    </condition>
                    <command>property-assign</command>
                    <property>/controls/engines/engine[1]/reverser</property>
                    <value type="bool">true</value>
                </binding>
                <binding>
                    <command>property-assign</command>
                    <property>/controls/engines/reverser-lock</property>
                    <value type="bool">true</value>
                </binding>
            </key>
  
            <key n="125">
                <name>|</name>
                <desc>Not Reverser on Selected Engine(s)</desc>
                <binding>
                    <condition>
                        <property>/sim/input/selected/engine[0]</property>
                    </condition>
                    <command>property-assign</command>
                    <property>/controls/engines/engine[0]/reverser</property>
                    <value type="bool">false</value>
                </binding>
                <binding>
                    <condition>
                        <property>/sim/input/selected/engine[1]</property>
                    </condition>
                    <command>property-assign</command>
                    <property>/controls/engines/engine[1]/reverser</property>
                    <value type="bool">false</value>
                </binding>
                <binding>
                    <condition>
                        <and>
                            <not>
                                <property>/controls/engines/engine[0]/reverser</property>
                            </not>
                            <not>
                                <property>/controls/engines/engine[1]/reverser</property>
                            </not>
                        </and>
                    </condition>
                    <command>property-assign</command>
                    <property>/controls/engines/reverser-lock</property>
                    <value type="bool">false</value>
                </binding>
            </key>

            <key n="44">
                <name>,</name>
                <desc>Left brake</desc>
                <binding>
                    <command>property-assign</command>
                    <property>/controls/gear/wheel[0]/brake-cmd</property>
                    <value type="double">1.0</value>
                </binding>
                <mod-up>
                    <binding>
                        <command>property-assign</command>
                        <property>/controls/gear/wheel[0]/brake-cmd</property>
                        <value type="double">0.0</value>
                    </binding>
                </mod-up>
            </key>

            <key n="46">
                <name>.</name>
                <desc>Right brake</desc>
                <binding>
                    <command>property-assign</command>
                    <property>/controls/gear/wheel[1]/brake-cmd</property>
                    <value type="double">1.0</value>
                </binding>
                <mod-up>
                    <binding>
                        <command>property-assign</command>
                        <property>/controls/gear/wheel[1]/brake-cmd</property>
                        <value type="double">0.0</value>
                    </binding>
                </mod-up>
            </key>

            <key n="98">
                <name>b</name>
                <desc>Apply all brakes.</desc>
                <binding>
                    <command>property-assign</command>
                    <property>/controls/gear/wheel[0]/brake-cmd</property>
                    <value type="double">1.0</value>
                </binding>
                <binding>
                    <command>property-assign</command>
                    <property>/controls/gear/wheel[1]/brake-cmd</property>
                    <value type="double">1.0</value>
                </binding>
                <binding>
                    <command>property-assign</command>
                    <property>/controls/gear/wheel[2]/brake-cmd</property>
                    <value type="double">1.0</value>
                </binding>
                <mod-up>
                    <desc>Release all brakes.</desc>
                    <binding>
                        <command>property-assign</command>
                        <property>/controls/gear/wheel[0]/brake-cmd</property>
                        <value type="double">0.0</value>
                    </binding>
                    <binding>
                        <command>property-assign</command>
                        <property>/controls/gear/wheel[1]/brake-cmd</property>
                        <value type="double">0.0</value>
                    </binding>
                    <binding>
                        <command>property-assign</command>
                        <property>/controls/gear/wheel[2]/brake-cmd</property>
                        <value type="double">0.0</value>
                    </binding>
                </mod-up>
            </key>

            <key n="66">
                <name>B</name>
                <desc>Toggle parking brake on or off</desc>
                <binding>
                    <command>property-toggle</command>
                    <property>/controls/gear/parking-brake-cmd</property>
                </binding>
            </key>

            <key n="81">
                <name>Q</name>
                <desc>Decrease LH propeller speed</desc>
                <binding>
                    <command>property-adjust</command>
                    <property>/controls/engines/engine[0]/propeller-pitch-cmd</property>
                    <min>0.0</min>
                    <max>1</max>
                    <step type="double">-0.2</step>
                </binding>
            </key>

            <key n="87">
                <name>W</name>
                <desc>Decrease RH propeller speed</desc>
                <binding>
                    <command>property-adjust</command>
                    <property>/controls/engines/engine[1]/propeller-pitch-cmd</property>
                    <min>0.0</min>
                    <max>1</max>
                    <step type="double">-0.2</step>
                </binding>
            </key>

            <key n="113">
                <name>q</name>
                <desc>Increase LH propeller speed</desc>
                <binding>
                    <command>property-adjust</command>
                    <property>/controls/engines/engine[0]/propeller-pitch-cmd</property>
                    <min>-0.2</min>
                    <max>1</max>
                    <step type="double">0.2</step>
                </binding>
            </key>

            <key n="119">
                <name>w</name>
                <desc>Increase RH propeller speed</desc>
                <binding>
                    <command>property-adjust</command>
                    <property>/controls/engines/engine[1]/propeller-pitch-cmd</property>
                    <min>-0.2</min>
                    <max>1</max>
                    <step type="double">0.2</step>
                </binding>
            </key>

            <key n="101">
                <name>e</name>
                <desc>Select pedal steering</desc>
                <binding>
                    <command>property-assign</command>
                    <property>/controls/switches/full-steering-sw</property>
                    <value>0</value>
                </binding>
            </key>
            <key n="69">
                <name>E</name>
                <desc>Select manual steering</desc>
                <binding>
                    <command>property-assign</command>
                    <property>/controls/switches/full-steering-sw</property>
                    <value>2</value>
                </binding>
            </key>

            <key n="47">
